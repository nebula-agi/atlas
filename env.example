# Copy this file to `.env` and fill in your keys.
# Never commit `.env` (it is gitignored).
#
# LLM provider (set at least one)
OPENROUTER_API_KEY=
GOOGLE_API_KEY=
OPENAI_API_KEY=

# Memory system keys (only needed if you evaluate those adapters)
MEM0_API_KEY=
SUPERMEMORY_API_KEY=
NEBULA_API_KEY=

# Optional: Nebula collection routing (advanced)
# NEBULA_COLLECTION_ID=
# NEBULA_SIMPLE_COLLECTION_ID=
# NEBULA_COMPLEX_COLLECTION_ID=
# NEBULA_COLLECTION_MODE=auto


